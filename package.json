  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn start --watch | pino-colada",
    "start:debug": "yarn start --debug --watch",
    "start:prod": "node dist/bundle.js",
    "webpack:build": "yarn prebuild && webpack --config webpack.config.js",
    "webpack:dev": "yarn prebuild && webpack --config webpack-hrm.config.js --watch | pino-pretty",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
